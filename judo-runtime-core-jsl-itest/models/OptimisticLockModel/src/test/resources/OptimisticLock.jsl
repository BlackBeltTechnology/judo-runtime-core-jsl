model OptimisticLock;

import judo::types;

entity Person {
    field String name;
}

entity Student extends Person {
    field String section;
}

transfer TransferPerson ( Person person ) {
    field String name <=> person.name;
}

transfer TransferStudent ( Student student ) {
    field String name <=> student.name;
    field String section <=> student.section;
}
