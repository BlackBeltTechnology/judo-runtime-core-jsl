model OptimisticLock;

import judo::types;

entity Person {
    field String name;
}

entity Student extends Person {
    field String section;
}

transfer TransferPerson ( Person person ) {
    field String name <= person.name bind:true;
}

transfer TransferStudent ( Student student ) {
    field String name <= student.name bind:true;
    field String section <= student.section bind:true;
}
