    field Integer rounded <= mapped.rounded input:true;
    field Integer unary <= mapped.unary input:true; // -6
    field Integer abs <= mapped.abs input:true;
    field Integer addition <= mapped.addition input:true;
    field Integer subtraction <= mapped.subtraction input:true;
