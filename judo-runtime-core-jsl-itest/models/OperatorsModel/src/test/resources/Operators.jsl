    field Boolean timeLt <= mapped.timeLt input:true;
    field Boolean timeLte <= mapped.timeLte input:true;
    field Boolean timeLte2 <= mapped.timeLte2 input:true;
    field Boolean timeGt <= mapped.timeGt input:true;
    field Boolean timeGte <= mapped.timeGte input:true;
    field Boolean timeGte2 <= mapped.timeGte2 input:true;
    field Boolean timeEq <= mapped.timeEq input:true;
    field Boolean timeNeq <= mapped.timeNeq input:true;

    field Boolean timestampEq <= mapped.timestampEq input:true;
    field Boolean timestampNeq <= mapped.timestampNeq input:true;

    field Boolean stringLt <= mapped.stringLt input:true;
    field Boolean stringLte <= mapped.stringLte input:true;
