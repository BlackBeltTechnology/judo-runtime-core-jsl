model CompositionRelationships;

type string String min-size:0 max-size:128;

entity EntityA {
    field String stringA;
    field EntityC singleConA;
    field EntityC[] collectionConA;
    field required EntityC singleRequiredConA;
}

entity abstract EntityB {
    field String stringB;
    field EntityD[] multipleDonB;
}

entity EntityC extends EntityB {
    field String stringC;
}

entity EntityD {
    field String stringD;
}

entity EntityE extends EntityD, EntityC {
    field String stringE;
}

entity EntityF {
  field EntityG g;
}

entity EntityG {
  field required String name default:EntityH!any().alwaysUndefined;
}

entity EntityH {
  field String alwaysUndefined;
}

entity EntityF2 {
  field EntityH h;
}

entity EntityF3 {
  field EntityG[] g;
}

entity EntityF4 {
  field EntityH[] h;
}

transfer TransferA(EntityA entitya) {
    field String stringA <= entitya.stringA input:true;
    relation TransferC singleConA <= entitya.singleConA eager:true choices:EntityC!all();
    relation TransferC[] collectionConA <= entitya.collectionConA eager:true choices:EntityC!all();
    relation required TransferC singleRequiredConA <= entitya.singleRequiredConA eager:true choices:EntityC!all();
}

transfer TransferC(EntityC entityc) {
    field String stringC <= entityc.stringC input:true;
    field String stringB <= entityc.stringB input:true;
    relation TransferD[] multipleDonB <= entityc.multipleDonB eager:true choices:EntityD!all();
}

transfer TransferD(EntityD entityd) {
    field String stringD <= entityd.stringD input:true;
}
