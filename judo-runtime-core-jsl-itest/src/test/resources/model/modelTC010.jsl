model modelTC010;

import judo::types;

entity EnvVars {
    field Boolean   fbool      = Boolean!getVariable(
                                     category = "ENVIRIONMENT",
                                     key = "JUDO_ENV_BOOLEAN1"
                                 );
    field Date      fdate      = Date!getVariable(
                                     category = "ENVIRIONMENT",
                                     key = "JUDO_ENV_DATE1"
                                 );
    field Time      ftime      = Time!getVariable(
                                     category = "ENVIRIONMENT",
                                     key = "JUDO_ENV_TIME1"
                                 );
    field Timestamp ftimestamp = Timestamp!getVariable(
                                     category = "ENVIRIONMENT",
                                     key = "JUDO_ENV_TIMESTAMP1"
                                 );
    field Integer   fint       = Integer!getVariable(
                                     category = "ENVIRIONMENT",
                                     key = "JUDO_ENV_INTEGER1"
                                 );
    field Long      flong      = Long!getVariable(
                                     category = "ENVIRIONMENT",
                                     key = "JUDO_ENV_LONG1"
                                 );
    field String    fstring    = String!getVariable(
                                     category = "ENVIRIONMENT",
                                     key = "JUDO_ENV_STRING1"
                                 );
}