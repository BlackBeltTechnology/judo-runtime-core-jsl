model ContainsModel;

type string String(min-size = 0, max-size = 255);
type boolean Boolean;

entity A {
    field B[] bs;
    field B b;
    derived Boolean containsTest => self.bs!contains(instance = ContainsModel::B!any());
    derived Boolean containsTest1 => self.bs!contains(instance = self.b);
}

entity B {
    field String name;
}
